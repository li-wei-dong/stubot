{
  "name": "Stubot",
  "description": "Stubot is a slackbot for the ages",
  "repository": "https://github.com/mmelcor/stubot",
  "logo": "http://informationtechnologysystems.com/wp-content/uploads/2016/01/cute-807306_960_720.png",
  "success_url": "/",
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    },
    "STARBOT_COMMAND_TOKEN": {
      "description": "Slash command token, for the starbot command endpoint",
      "required": true
    },
    "SLACK_TOKEN": {
      "description": "Slack bot RTM API token",
      "required": false
    },
    "NODE_ENV": "staging"
  },
  "image": "heroku/nodejs"
}
